                                                    @Html.TextBoxFor(x => x.FrDt, new { @class = "form-control form-control-lg", @placeholder = "From Date", @tabindex = "7", @style = "font-family: Verdana;font-size:small; height:30px; width:180px", @type = "date" })
                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                <label class="control-label font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                    To Dt. <span class="required"></span>
                                                </label>
                                                <div class="form-outline form-white mb-2">
                                                    @Html.TextBoxFor(x => x.ToDt, new { @class = "form-control form-control-lg", @placeholder = "To Date", @tabindex = "8", @style = "font-family: Verdana;font-size:small; height:30px; width:180px", @type = "date" })
                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                <label class="control-label font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                    V.No <span class="required"></span>
                                                </label>
                                                <div class="form-outline form-white mb-2">
                                                    @Html.TextBoxFor(x => x.FltVno, new { @class = "form-control form-control-lg", @placeholder = "VNo", @tabindex = "2", @style = "font-family: Verdana;font-size:small; height:30px; width:180px", @type = "text" })
                                                    @*@Html.TextBoxFor(x => x.FltOD, new { @class = "form-control form-control-lg", @placeholder = "OD", @tabindex = "2", @style = "font-family: Verdana;font-size:small; height:30px; width:180px", @type = "text" })*@                                                    
                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                                </div>
                                            <div class="col-md-1 mb-2 float-left">

                                                <select class="form-control form-control-sm" onchange="loadData()" id="drpReportPageNoDatable">
                                                    @for (int i = 0; i < pageNoList.Count; i++)
                                                    {
                                                        string checkedd = "checked";
                                                        <option checked="@checkedd" value="@pageNoList[i].Value">@pageNoList[i].Text</option>
                                                        checkedd = string.Empty;
                                                    }

                                                </select>

                                            </div>
                                            <div class="col-md-7 col-12">
                                                <div class="row">
                                                    <div class="col-md-3 col">
                                                        <select class="form-control select2 drpReportLayout">
                                                            <option value="" ; style="width:200px">--Select Layout--</option>
                                                            @if (layoutList != null && layoutList.Count > 0)
                                                            {
                                                                foreach (var item in layoutList)
                                                                {
                                                                    <option value="@item.Value">@item.Text </option>
                                                                }
                                                            }
                                                        </select>
                                                    </div>
                                                    <div class="col-md-2 col-auto mb-2 custommt-2-3">
                                                        @if (isAdministrator == 1)
                                                        {
                                                            <button class="btn btn-dark btn-sm text-white" style="font-family:Verdana;font-size:medium" onclick="window.open('/GridMaster/Index/' + $('.drpReportLayout').val(), '_blank');">
                                                                Edit
                                                            </button>
                                                        }
                                                    </div>
                                                    <div class="col-md-7 mb-2 custommt-2-3">
                                                        <button class="btn btn-primary btn-sm" id="btnExportToExcel">Excel</button>
                                                        <button class="btn btn-primary btn-sm ml-6" id="btnExportToPDF">PDF</button>
                                                        <button class="btn btn-primary btn-sm ml-6" id="btnView" onclick=" loadData();">View (Alt+V)</button>
                                                        <a href="/JobWorkEntry/index" class="btn btn-warning btn-sm ml-6">
                                                            Cancel (Esc)
                                                        </a>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-md-1"></div>
                                            <div class="col-md-3 col-12 float-right">
                                                <input type="text" class="form-control form-control-sm" placeholder="Search" id="txtReportSearchDataTable" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="dataTable">

                                    </div>
                                    <div class="pager float-right"></div>
                                </div>
                            </div>
                        }
                        @if (userRight != null && (userRight.IsAdd || userRight.IsEdit))
                        {
                            <div class="tab-pane fade @gridAdd" id="custom-tabs-three-add" role="tabpanel" aria-labelledby="custom-tabs-three-add-tab">
                                <form method="post" class="mt-1" id="straightingForm">
                                    <input type="hidden" id="txtIsPrint" name="isPrint" value="0" />
                                    <div class="container-fluid">
                                        <div class="row">
                                            <div class="d-none" id="dropdownGrade">
                                                @if (Model != null && Model.Straighting != null && Model.Straighting.GradeList != null && Model.Straighting.GradeList.Count > 0)
                                                {
                                                    @Html.DropDownListFor(x => x.Straighting.GradeVou, new SelectList(Model.Straighting.GradeList, "Value", "Text"), "Select", new { @class = "form-control", @style = "font-family: Verdana;font-size:small; height:30px;" })
                                                }

                                            </div>
                                            <div class="d-none" id="dropdownRecProduct">
                                                @if (Model != null && Model.Straighting != null && Model.Straighting.RecProductList != null && Model.Straighting.RecProductList.Count > 0)
                                                {
                                                    @Html.DropDownListFor(x => x.Straighting.RecPrdVou, new SelectList(Model.Straighting.RecProductList, "Value", "Text"), "Select", new { @class = "form-control", @style = "font-family: Verdana;font-size:small; height:30px;" })
                                                }

                                            </div>
                                            <div class="d-none" id="dropdownNB">
                                                @if (Model != null && Model.Straighting != null && Model.Straighting.NBList != null && Model.Straighting.NBList.Count > 0)
                                                {
                                                    @Html.DropDownListFor(x => x.Straighting.NbVou, new SelectList(Model.Straighting.NBList, "Value", "Text"), "Select", new { @class = "form-control", @style = "font-family: Verdana;font-size:small; height:30px;" })
                                                }

                                            </div>
                                            <div class="d-none" id="dropdownSCH">
                                                @if (Model != null && Model.Straighting != null && Model.Straighting.SCHList != null && Model.Straighting.SCHList.Count > 0)
                                                {
                                                    @Html.DropDownListFor(x => x.Straighting.SchVou, new SelectList(Model.Straighting.SCHList, "Value", "Text"), "Select", new { @class = "form-control", @style = "font-family: Verdana;font-size:small; height:30px;" })
                                                }
                                            </div>
                                            <div class="d-none" id="dropdownStatus">
                                                @if (Model != null && Model.Straighting != null && Model.Straighting.StatusList != null && Model.Straighting.StatusList.Count > 0)
                                                {
                                                    @Html.DropDownListFor(x => x.Straighting.StatusVou, new SelectList(Model.Straighting.StatusList, "Value", "Text"), "Select", new { @class = "form-control", @style = "font-family: Verdana;font-size:small; height:30px;" })
                                                }

                                            </div>
                                            <div class="d-none" id="dropdownFinish">
                                                @if (Model != null && Model.Straighting != null && Model.Straighting.FinishList != null && Model.Straighting.FinishList.Count > 0)
                                                {
                                                    @Html.DropDownListFor(x => x.Straighting.FinishVou, new SelectList(Model.Straighting.FinishList, "Value", "Text"), "Select", new { @class = "form-control", @style = "font-family: Verdana;font-size:small; height:30px;" })
                                                }

                                            </div>
                                            <div class="d-none" id="dropdownNextProc">
                                                @if (Model != null && Model.Straighting != null && Model.Straighting.NextProcList != null && Model.Straighting.NextProcList.Count > 0)
                                                {
                                                    @Html.DropDownListFor(x => x.Straighting.NextProcVou, new SelectList(Model.Straighting.NextProcList, "Value", "Text"), "Select", new { @class = "form-control", @style = "font-family: Verdana;font-size:small; height:30px;" })
