    function GetLotIssStrlingProduct(previousRow, isExists) {
        if (previousRow != null) {
            var Strvou = '@Html.Raw(Model.StrVou)';
            var dt = $('#Date').val();
            var recProdId = $(previousRow).closest('tr').children('td').eq(1).children('select').val();
            var gradeId = $(previousRow).closest('tr').children('td').eq(2).children('select').val();
            var finishId = $(previousRow).closest('tr').children('td').eq(3).children('select').val();
            var nbId = $(previousRow).closest('tr').children('td').eq(4).children('select').val();
            var schId = $(previousRow).closest('tr').children('td').eq(5).children('select').val();
            var langth = $(previousRow).closest('tr').children('td').eq(6).children('input').val();
            var od = $(previousRow).closest('tr').children('td').eq(7).children('input').val();
            var thick = $(previousRow).closest('tr').children('td').eq(8).children('input').val();
            var pcs = $(previousRow).closest('tr').children('td').eq(9).children('input').val();
            var gsrno = $(previousRow).closest('tr').children('td').eq(0).children('input').val();
            if (gradeId != "" && od != "") {
                $.ajax({
                    url: '/Straighting/GetLotIssStraightProduct',
                    type: 'POST',
                    data: { 'recProdId': recProdId, 'strvou': Strvou, 'gradeId': gradeId, 'nbId': nbId, 'schId': schId, 'od': od, 'dt': dt, "gsrno": gsrno, "thick": thick, "langth": langth, "finishId": finishId },
                    dataType: 'JSON',
                    traditional: true, // add this
                    success: function (data) {
                        if (data.result) {
                            if (data.data == "1" && !isExists) {
                                if (nbId != "" && schId != "") {
                                    showErrorMessage("Pickling OD Size Not Exsits!");
                                    $(previousRow).closest('tr').children('td').eq(7).children('input').val('');
                                    $(previousRow).closest('tr').children('td').eq(8).children('input').val('');
                                    $(previousRow).closest('tr').children('td').eq(9).children('input').val('');
                                    $(previousRow).closest('tr').children('td').eq(3).children('input').focus();
                                }
                            }
                            else if (data.data == "2" && !isExists) {
                                showErrorMessage("Pickling Date Should be Grater Then!");
                                $(previousRow).closest('tr').children('td').eq(7).children('input').val('');
                                $(previousRow).closest('tr').children('td').eq(8).children('input').val('');
                                $(previousRow).closest('tr').children('td').eq(9).children('input').val('');
                                document.getElementById('Date').focus();
                            }
                            else if (data.data == "3" && !isExists) {
                                $(previousRow).closest('tr').children('td').eq(7).children('input').focus();
                            }
                            else {
                                if (nbId != "" && schId != "") {
                                    $(previousRow).closest('tr').children('td').eq(8).children('input').val(data.lotThick);
                                    $(previousRow).closest('tr').children('td').eq(9).children('input').val(data.lotPcs);
                                    $(previousRow).closest('tr').children('td').eq(10).children('input').val(data.lotQty);
                                    var length = $(previousRow).closest('tr').children('td').eq(6).children('input').val();
                                    var lengthmtr = (length * 0.3048).toFixed(4);
                                    var weight = (((od) - thick) * thick * 0.02503 * lengthmtr).toFixed(2);
                                    var weight2 = (((od) - thick) * thick * 0.02503 * lengthmtr).toFixed(8);
                                    var pcs = $(previousRow).closest('tr').children('td').eq(9).children('input').val();
                                    $(previousRow).closest('tr').children('td').eq(10).children('input').val((weight2 * pcs).toFixed(1));
                                }
                                else {
                                    $(previousRow).closest('tr').children('td').eq(9).children('input').val(data.lotPcs);
                                    $(previousRow).closest('tr').children('td').eq(10).children('input').val(data.lotQty);
                                    var length = $(previousRow).closest('tr').children('td').eq(6).children('input').val();
                                    var lengthmtr = (length * 0.3048).toFixed(4);
                                    var weight = (((od) - thick) * thick * 0.02503 * lengthmtr).toFixed(2);
                                    var weight2 = (((od) - thick) * thick * 0.02503 * lengthmtr).toFixed(8);
                                    var pcs = $(previousRow).closest('tr').children('td').eq(9).children('input').val();
                                    $(previousRow).closest('tr').children('td').eq(10).children('input').val((weight2 * pcs).toFixed(1));

                                }
                            }
                        }
                        else {
                            errorMessage(response);
                        }
                    },
                    failure: function (response) {
                        errorMessage(response);
                    }
                });
            }
        }
    }

