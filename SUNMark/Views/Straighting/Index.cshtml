                                                            @Html.DropDownListFor(x => x.MachineNo, new SelectList(milprocessList, "Value", "Text"), "Select", new { @class = "form-control select2 w-100", @style = "width:100% !important", @onchange = "GetSupervisor()" })

                                                        </div>
                                                    </div>
                                                    <div class="col-md-2 col-6">
