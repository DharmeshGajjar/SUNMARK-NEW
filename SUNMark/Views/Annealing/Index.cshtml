    function GetLotIssAnnelProduct(previousRow, isExists) {
        
        if (previousRow != null) {
            var annvou = '@Html.Raw(Model.AnnVou)';
            var dt = $('#Date').val();
            var recProdId = $(previousRow).closest('tr').children('td').eq(1).children('select').val();
            var gradeId = $(previousRow).closest('tr').children('td').eq(2).children('select').val();
            var nbId = $(previousRow).closest('tr').children('td').eq(3).children('select').val();
            var schId = $(previousRow).closest('tr').children('td').eq(4).children('select').val();
            var finishId = $('#FinishVou').val();
            var od = $(previousRow).closest('tr').children('td').eq(5).children('input').val();
            var thick = $(previousRow).closest('tr').children('td').eq(6).children('input').val();
            var langth = $(previousRow).closest('tr').children('td').eq(7).children('input').val();
            var pcs = $(previousRow).closest('tr').children('td').eq(8).children('input').val();
            var gsrno = $(previousRow).closest('tr').children('td').eq(0).children('input').val();
            if (gradeId != "" && od != "" && (pcs == "0" || pcs == "") ) {
                $.ajax({
                    url: '/Annealing/GetLotIssAnnelProduct',
                    type: 'POST',
                    data: { 'recProdId': recProdId, 'annvou': annvou, 'gradeId': gradeId, 'nbId': nbId, 'schId': schId, 'od': od, 'dt': dt, "gsrno": gsrno, "thick": thick, "langth": langth, "finishId": finishId },
                    dataType: 'JSON',
                    traditional: true, // add this
                    success: function (data) {
                        if (data.result) {
                            if (data.data == "1" && !isExists) {
                                if (nbId != "" && schId != "") {
                                    showErrorMessage("Annealing OD Size Not Exsits!");
                                    $(previousRow).closest('tr').children('td').eq(8).children('input').val('');
                                    $(previousRow).closest('tr').children('td').eq(9).children('input').val('');
                                    $(previousRow).closest('tr').children('td').eq(20).children('input').val('');
                                    $(previousRow).closest('tr').children('td').eq(3).children('input').focus();
                                }
                            }
                            else if (data.data == "2" && !isExists) {
                                showErrorMessage("Annealing Date Should be Grater Then!");
                                $(previousRow).closest('tr').children('td').eq(7).children('input').val('');
                                $(previousRow).closest('tr').children('td').eq(8).children('input').val('');
