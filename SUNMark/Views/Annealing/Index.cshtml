                                //$(previousRow).closest('tr').children('td').eq(20).children('input').val('');
                                document.getElementById('Date').focus();
                            }
                            else if (data.data == "3" && !isExists) {
                                $(previousRow).closest('tr').children('td').eq(7).children('input').focus();
                            }
                            else {
                                if (nbId != "" && schId != "") {
                                    $(previousRow).closest('tr').children('td').eq(7).children('input').val("20");
                                    $(previousRow).closest('tr').children('td').eq(6).children('input').val(data.lotThick);
                                    $(previousRow).closest('tr').children('td').eq(8).children('input').val(data.lotPcs);
