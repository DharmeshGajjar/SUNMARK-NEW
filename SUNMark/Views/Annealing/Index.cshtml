                                    //$(previousRow).closest('tr').children('td').eq(20).children('input').val(data.lotvou);
                                    var length = $(previousRow).closest('tr').children('td').eq(7).children('input').val();
                                    var lengthmtr = (length * 0.3048).toFixed(4);
                                    var weight = (((od) - thick) * thick * 0.02503 * lengthmtr).toFixed(2);
                                    var weight2 = (((od) - thick) * thick * 0.02503 * lengthmtr).toFixed(8);
                                    var pcs = $(previousRow).closest('tr').children('td').eq(8).children('input').val();
                                    $(previousRow).closest('tr').children('td').eq(9).children('input').val((weight2 * pcs).toFixed(1));
                                }
                                else {
                                    $(previousRow).closest('tr').children('td').eq(8).children('input').val(data.lotPcs);
                                    $(previousRow).closest('tr').children('td').eq(9).children('input').val(data.lotQty);
