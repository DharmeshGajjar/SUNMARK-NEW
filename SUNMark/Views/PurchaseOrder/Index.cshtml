<script>

    function Calculation(obj, isTrue) {
        var qtyIn = $('#OrdQtyIn').val();
        var length = $(obj).closest('tr').children('td').eq(9).children('input').val()
        var qty = $(obj).closest('tr').children('td').eq(10).children('input').val()
        if (qtyIn) {
            if (qtyIn == 'FOOT') {
                var total = (parseFloat(qty) / 3.28084 / parseFloat(length));
                $(obj).closest('tr').children('td').eq(11).children('input').val(total.toFixed(2))
            }
            else if (qtyIn == 'METER') {
                var total = parseFloat(qty) / parseFloat(length);
                $(obj).closest('tr').children('td').eq(11).children('input').val(total.toFixed(2))
            }
            else if (qtyIn == 'PCS') {
                $(obj).closest('tr').children('td').eq(11).children('input').val(qty.toFixed(2))
            }
        }
    }

