@model SUNMark.Models.OpeningStokPipeModel
@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    int isAdministrator = HttpContextAccessor.HttpContext.Session.GetInt32("IsAdministrator").Value;
    List<SUNMark.Models.CityModel> cityList = ViewBag.cityList as List<SUNMark.Models.CityModel>;
    List<SelectListItem> layoutList = ViewBag.layoutList as List<SelectListItem>;
    List<SelectListItem> pageNoList = ViewBag.pageNoList as List<SelectListItem>;
    List<SelectListItem> nbList = ViewBag.nbList != null ? ViewBag.nbList as List<SelectListItem> : null;
    List<SelectListItem> schList = ViewBag.schList != null ? ViewBag.schList as List<SelectListItem> : null;
    var userRight = ViewBag.userRight as SUNMark.Models.UserFormRightModel;
    ViewData["Title"] = userRight.ModuleNm;
    string headerAdd = "";
    string headerList = "";
    string gridAdd = "";
    string gridList = "";
    bool headerAddTrue = false;
    bool headerListTrue = false;

    if (userRight != null && (userRight.IsAdd || userRight.IsEdit) && userRight.IsList)
    {
        headerList = "active";
        gridList = "show active";
        headerAddTrue = true;
    }
    else if (userRight != null && (userRight.IsAdd || userRight.IsEdit))
    {
        headerAdd = "active";
        gridAdd = "show active";
        headerAddTrue = true;
    }
    else if (userRight != null && userRight.IsList)
    {
        headerList = "active";
        gridList = "show active";
        headerAddTrue = true;
    }
}

<style>
    .clsrequied {
        color: red;
    }

    .lblstyle {
        font-family: Verdana;
        font-size: small
    }
</style>
<div class="card card-primary">
    <div class="card-header" style="font-family:Verdana">
        <h6> @ViewData["Title"]</h6>
    </div>
    <div class="card-body">
        <div class="card card-primary card-tabs">
            <div class="card-header p-0 pt-1">
                <ul class="nav nav-tabs" id="custom-tabs-three-tab" role="tablist">
                    @if (userRight != null && userRight.IsList)
                    {
                        <li class="nav-item">
                            <a class="nav-link @headerList" id="custom-tabs-three-list-tab" data-toggle="pill" href="#custom-tabs-three-list" role="tab" aria-controls="custom-tabs-three-list" aria-selected="@headerListTrue" style="font-family:Verdana;font-size:small" onclick="GoToListTab()">List (Alt+L)</a>
                        </li>
                    }
                    @if (userRight != null && (userRight.IsAdd || userRight.IsEdit))
                    {
                        <li class="nav-item">
                            <a class="nav-link @headerAdd" id="custom-tabs-three-add-tab" data-toggle="pill" href="#custom-tabs-three-add" role="tab" aria-controls="custom-tabs-three-add" aria-selected="@headerAddTrue" style="font-family:Verdana;font-size:small" onclick="GoToAddTab()">Add/Update (F2)</a>
                        </li>
                    }
                </ul>
            </div>
            <div class="card-body">
                <div class="col-md-12">
                    <div class="tab-content" id="custom-tabs-three-tabContent">
                        @if (userRight != null && userRight.IsList)
                        {
                            <div class="tab-pane fade  @gridList" id="custom-tabs-three-list" role="tabpanel" aria-labelledby="custom-tabs-three-list-tab">
                                <div class="table-responsive mt-2" style="font-family:Verdana;font-size:small;font-display:inherit">
                                    <div class="col-md-12 col-12 mb-2 mt-2">
                                        <div class="row">
                                            <div class="col-md-1 mb-2 float-left">
                                                <select class="form-control form-control-sm" onchange="loadData()" id="drpReportPageNoDatable">
                                                    @for (int i = 0; i < pageNoList.Count; i++)
                                                    {
                                                        string checkedd = "checked";
                                                        <option checked="@checkedd" value="@pageNoList[i].Value">@pageNoList[i].Text</option>
                                                        checkedd = string.Empty;
                                                    }

                                                </select>
                                            </div>
                                            <div class="col-md-5 col-12">
                                                <div class="row">
                                                    <div class="col-md-6 col">
                                                        <select class="form-control select2 drpReportLayout" onchange="loadData()">
                                                            <option value="" ; style="width:200px">--Select Layout--</option>
                                                            @if (layoutList != null && layoutList.Count > 0)
                                                            {
                                                                foreach (var item in layoutList)
                                                                {
                                                                    <option value="@item.Value">@item.Text </option>
                                                                }
                                                            }
                                                        </select>
                                                    </div>
                                                    <div class="col-md-2 col-auto mb-2 custommt-2-3">
                                                        @if (isAdministrator == 1)
                                                        {
                                                            <button class="btn btn-dark btn-sm text-white" style="font-family:Verdana;font-size:medium" onclick="window.open('/GridMaster/Index/' + $('.drpReportLayout').val(), '_blank');">
                                                                Edit
                                                            </button>
                                                        }
                                                    </div>
                                                    <div class="col-md-4 mb-2 custommt-2-3">
                                                        <button class="btn btn-primary btn-sm" id="btnExportToExcel">Excel</button>
                                                        <button class="btn btn-primary btn-sm ml-6" id="btnExportToPDF">PDF</button>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-md-3"></div>
                                            <div class="col-md-3 col-12 float-right">
                                                <input type="text" class="form-control form-control-sm" placeholder="Search" id="txtReportSearchDataTable" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="dataTable">

                                    </div>
                                    <div class="pager float-right"></div>
                                </div>
                            </div>
                        }
                        @if (userRight != null && (userRight.IsAdd || userRight.IsEdit))
                        {
                            <div class="tab-pane fade @gridAdd" id="custom-tabs-three-add" role="tabpanel" aria-labelledby="custom-tabs-three-add-tab">
                                <form method="post" class="mt-1">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            V.No <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-auto col-12 form-outline form-white mb-2">
                                                            @Html.TextBoxFor(x => x.OblNVno, new { @class = "form-control form-control-lg", @placeholder = "Bilty No", @tabindex = "1", @style = "font-family: Verdana;font-size:small; height:30px; width: 100px", @required = "required", @name = "OblNVno" })
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">

                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Date <span class="required clsrequied">*</span>
                                                        </label>

                                                        <div class="col-md-auto col-12 form-outline form-white mb-2">
                                                            @Html.TextBoxFor(x => x.OblDt, new { @class = "form-control form-control-lg", @placeholder = "Date", @tabindex = "2", @style = "font-family: Verdana;font-size:small; height:30px; width: 200px", @required = "required", @type = "date" })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Company <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-9 d-flex form-outline form-white mb-2" tabindex=" = 2">
                                                            @Html.DropDownListFor(x => x.OblCmpVou, Model.OblCmpVouList, "Select", new { @class = "form-control select2 OblCmpVou", @tabindex = "3", onfocus = "this.select()", style = "font-family: Verdana;font-size:small; height:30px; width:300px", @required = "required", @ID = "txtOblCmpVou" })
                                                            <a class="btn btn-outline-info ml-2" href="/Department/index" target="_blank"><i class="fa fa-plus"></i></a>
                                                            <a class="btn btn-outline-info ml-2" onclick="GetCompanyMasterDropdown()"><i class="fa fa-sync"></i></a>
                                                            @*<a class="btn btn-outline-info ml-2" onclick="OpenStateModal()"><i class="fa fa-plus"></i></a>*@
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Godown <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-9 d-flex form-outline form-white mb-2">
                                                            @Html.DropDownListFor(x => x.OblGdnVou, Model.OblGdnVouList, "Select", new { @class = "form-control select2 OblGdnVou", @tabindex = "4", onfocus = "this.select()", style = "font-family: Verdana;font-size:small; height:30px; width:300px", @required = "required", @ID = "txtOblGdnVou" })
                                                            <a class="btn btn-outline-info ml-2" href="/GodownMaster/index" target="_blank"><i class="fa fa-plus"></i></a>
                                                            <a class="btn btn-outline-info ml-2" onclick="GetGoDownMasterDropdown()"><i class="fa fa-sync"></i></a>
                                                            @*<a class="btn btn-outline-info ml-2" onclick="OpenStateModal()"><i class="fa fa-plus"></i></a>*@
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">

                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Location <span class="required clsrequied"></span>
                                                        </label>
                                                        <div class="col-md-9 d-flex form-outline form-white mb-2">
                                                            @Html.DropDownListFor(x => x.OblLocVou, Model.OblLocVouList, "Select", new { @class = "form-control select2 OblLocVou", @tabindex = "5", onfocus = "this.select()", style = "font-family: Verdana;font-size:small; height:30px; width:300px", @ID = "txtOblLocVou" })
                                                            <a class="btn btn-outline-info ml-2" href="/Location/index" target="_blank"><i class="fa fa-plus"></i></a>
                                                            <a class="btn btn-outline-info ml-2" onclick="GetLocationMasterDropdown()"><i class="fa fa-sync"></i></a>
                                                            @*<a class="btn btn-outline-info ml-2" onclick="OpenStateModal()"><i class="fa fa-plus"></i></a>*@
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Heat No. <span class="required clsrequied"></span>
                                                        </label>
                                                        <div class="col-md-auto col-12 form-outline form-white mb-2">
                                                            <input type="text" id="LotHeatNo" name="LotHeatNo" class="form-control form-control-lg" placeholder="Heat No." tabindex="6" value="@Model.LotHeatNo" style="font-family: Verdana;font-size:small; height:30px;width:150px" />
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Product <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-9 d-flex form-outline form-white mb-2" tabindex=" = 2">
                                                            @Html.DropDownListFor(x => x.OblPrdVou, Model.OblPrdVouList, "Select", new { @class = "form-control select2 OblPrdVou", @tabindex = "7", onfocus = "this.select()", style = "font-family: Verdana;font-size:small; height:30px; width:300px", @required = "required", @ID = "txtOblPrdVou" })
                                                            <a class="btn btn-outline-info ml-2" href="/ProductMaster/index" target="_blank"><i class="fa fa-plus"></i></a>
                                                            <a class="btn btn-outline-info ml-2" onclick="GetProductMasterDropdown()"><i class="fa fa-sync"></i></a>
                                                            @*<a class="btn btn-outline-info ml-2" onclick="OpenStateModal()"><i class="fa fa-plus"></i></a>*@
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Process Done <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-9 d-flex form-outline form-white mb-2" tabindex=" = 2">
                                                            @Html.DropDownListFor(x => x.LotPrcTypVou, Model.LotPrcTypVouList, "Select", new { @class = "form-control select2 LotPrcTypVou", @tabindex = "8", onfocus = "this.select()", style = "font-family: Verdana;font-size:small; height:30px; width:300px", @required = "required", @ID = "txtLotPrcTypVou" })
                                                            <a class="btn btn-outline-info ml-2" href="#" target="_blank"><i class="fa fa-plus"></i></a>
                                                            <a class="btn btn-outline-info ml-2" onclick="GetLotPrcTypMasterDropdown()"><i class="fa fa-sync"></i></a>
                                                            @*<a class="btn btn-outline-info ml-2" onclick="OpenStateModal()"><i class="fa fa-plus"></i></a>*@
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Grade <span class="required clsrequied">*</span>
                                                        </label>
                                                        <input type="hidden" name="Grade" id="Grade" />
                                                        <div class="col-md-9 d-flex form-outline form-white mb-2" tabindex=" = 2">
                                                            @Html.DropDownListFor(x => x.LotGrade, Model.LotGradeList, "Select", new { @class = "form-control select2 LotGrade", @tabindex = "9", onfocus = "this.select()", style = "font-family: Verdana;font-size:small; height:30px; width:300px", @required = "required", @ID = "txtLotGrade" })
                                                            <a class="btn btn-outline-info ml-2" href="/GradeMaster/index" target="_blank"><i class="fa fa-plus"></i></a>
                                                            <a class="btn btn-outline-info ml-2" onclick="GetGradeMasterDropdown()"><i class="fa fa-sync"></i></a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Width <span class="required clsrequied"></span>
                                                        </label>
                                                        <div class="col-md-auto col-12 form-outline form-white mb-2">
                                                            <input type="text" id="LotWidth" name="LotWidth" class="form-control form-control-lg" placeholder="Width " tabindex="10" value="@Model.LotWidth" style="font-family: Verdana;font-size:small; height:30px;width:150px;text-align:right" onchange="GetODDetails(this)"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Thick <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-auto col-12 form-outline form-white mb-2">
                                                            <input type="text" id="LotThick" name="LotThick" class="form-control form-control-lg" placeholder="Thick " tabindex="11" value="@Model.LotThick" style="font-family: Verdana;font-size:small; height:30px;width:150px;text-align:right" @*onchange="GetNBSCHDetails(this)"*@ required />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            OD <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-auto col-12 form-outline form-white mb-2">
                                                            <input type="text" id="LotOD" name="LotOD" class="form-control form-control-lg" placeholder="OD" tabindex="12" value="@Model.LotOD" style="font-family: Verdana;font-size:small; height:30px;width:150px;text-align:right" @*onchange="GetNBSCHDetails(this)"*@ required />
                                                            <label class="control-label pl-1" style="font-family: Verdana;font-size:small; height:30px;" id="ODVal"></label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            NB <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-auto col-12 form-outline form-white mb-2" style="width: 29% !important;">
                                                            @*<input type="text" id="LotNB" name="LotNB" class="form-control form-control-lg" placeholder="NB" tabindex="13" value="@Model.LotNB" style="font-family: Verdana;font-size:small; height:30px;width:150px" required />*@
