                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            SCH <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-auto col-12 form-outline form-white mb-2" style="width: 29% !important;">
                                                            @*<input type="text" id="LotSCH" name="LotSCH" class="form-control form-control-lg" placeholder="SCH" tabindex="14" value="@Model.LotSCH" style="font-family: Verdana;font-size:small; height:30px;width:150px" required />*@
                                                            @Html.DropDownListFor(x => x.LotSCH, schList, "Select", new { @class = "form-control select2", @style = "width:100% ", @required = "required" })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Feet Per <span class="required clsrequied">*</span>
                                                        </label>
                                                        <div class="col-md-auto col-12 form-outline form-white mb-2">
                                                            <input type="text" id="FeetPer" name="FeetPer" class="form-control form-control-lg" placeholder="Length" tabindex="15" value="@Model.FeetPer" style="font-family: Verdana;font-size:small; height:30px;width:150px;text-align:right" required />
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="control-label col-md-3 col-sm-3 font-weight-bold customLabel" style="font-family:Verdana;font-size:small">
                                                            Finish <span class="required clsrequied"></span>
                                                        </label>
                                                        <input type="hidden" name="Finish" id="Finish" />
                                                        <div class="col-md-9 d-flex form-outline form-white mb-2" tabindex=" = 2">
                                                            @Html.DropDownListFor(x => x.LotFinish, Model.LotFinishList, "Select", new { @class = "form-control select2 LotFinish", @tabindex = "16", onfocus = "this.select()", style = "font-family: Verdana;font-size:small; height:30px; width:300px", @ID = "txtLotFinish" })
                                                            <a class="btn btn-outline-info ml-2" href="/FinishMaster/index" target="_blank"><i class="fa fa-plus"></i></a>
                                                            <a class="btn btn-outline-info ml-2" onclick="GetFinishMasterDropdown()"><i class="fa fa-sync"></i></a>
